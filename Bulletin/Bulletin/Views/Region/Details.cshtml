@using Bulletin.Models

@{
    ViewBag.Title = "Административный район или населённый пункт";
}



<h2>Просмотр Административного района или населённого пункта</h2>

    <fieldset>
        <legend>Поля</legend>
        <p>
            Наименование:
            @Html.Encode(Model.Name)
        </p>
        <p>
            Градация по ветру:
            @Html.Encode(Model.Deltawind)
        </p>
        <p>
            Градация по температуре:
            @Html.Encode(Model.Deltatemperature)
        </p>        
        
    </fieldset>
    <p>
        @Html.ActionLink("Редактировать", "Edit", new { id = Model.ID }) |
        @Html.ActionLink("Назад", "Index")
    </p>